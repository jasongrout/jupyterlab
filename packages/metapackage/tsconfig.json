{
  "compilerOptions": {
    "declaration": true,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES2015",
    "outDir": "./lib",
    "lib": ["ES2015", "DOM"],
    "baseUrl": ".",
    "paths": {
      "@jupyterlab/*": [
        "../*/src"
      ],
      "@jupyterlab/mainmenu-extension": [
        "../mainmenu-extension/src"
      ]
    },
    "jsx": "react",
    "inlineSources": true,
    "sourceMap": true
  }
}
